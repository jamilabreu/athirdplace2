if ( $("#<%= @user.id %>_profile").length ) {
	$("#<%= @user.id %>_profile").toggle();
} else {
	$("#<%= @user.id %>").append("<%= j render partial: 'users/profile', locals: { user: @user } %>");
}
$('#<%= @user.id %>_image i').toggleClass('icon-chevron-down icon-chevron-up');
//$('#users').masonry('reload');
$('.user').wookmark({
	container: $('#users'),
	offset: 10,
	itemWidth: 192,
	autoResize: true	
});
